	.data
	
array:	.space 24
insert:	.asciiz "Insira 1 valor: "	
	
	.text
	.globl main
	
main:
	li $t0,0
	la $s0,array
	
fill:	beq $t0,6,efill #Fill tem como objetivo preencher o array
	
	la $a0, insert
	li $v0, 4
	syscall
	
	li $v0, 5
	syscall
	
	sll $t1, $t0,2
	add $t2, $a0,$s0
	sw $v0,0($t2)
	
	addi $t0,$t0,1
	jfillp
	